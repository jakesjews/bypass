{
  "name": "medplum-chart-demo",
  "version": "5.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "backend": "docker compose up",
    "build": "pnpm run build:bots && tsc && vite build",
    "build:bots": "pnpm run clean && tsc --project tsconfig-bots.json && node --no-warnings esbuild-script.mjs && node src/scripts/deploy-bots.mjs",
    "clean": "node -e \"require('node:fs').rmSync('dist', { recursive: true, force: true })\"",
    "dev": "vite",
    "format": "prettier --write \"{src,.github}/**/*.{ts,tsx,js,mjs,cjs,json,css,yml,yaml}\" package.json pnpm-workspace.yaml docker-compose.yml esbuild-script.mjs eslint.config.cjs postcss.config.mjs tsconfig.json tsconfig-bots.json vercel.json",
    "format:check": "prettier --check \"{src,.github}/**/*.{ts,tsx,js,mjs,cjs,json,css,yml,yaml}\" package.json pnpm-workspace.yaml docker-compose.yml esbuild-script.mjs eslint.config.cjs postcss.config.mjs tsconfig.json tsconfig-bots.json vercel.json",
    "typecheck": "tsc --noEmit && tsc --project tsconfig-bots.json --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@mantine/core": "8.3.15",
    "@mantine/hooks": "8.3.15",
    "@mantine/notifications": "8.3.15",
    "@medplum/core": "5.0.14",
    "@medplum/definitions": "5.0.14",
    "@medplum/fhirtypes": "5.0.14",
    "@medplum/mock": "5.0.14",
    "@medplum/react": "5.0.14",
    "@tabler/icons-react": "3.36.1",
    "@types/node": "25.2.3",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "@vitejs/plugin-react": "5.1.4",
    "eslint": "10.0.0",
    "esbuild": "0.27.3",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "postcss": "8.5.6",
    "postcss-preset-mantine": "1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "3.6.2",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-router": "7.13.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  },
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc",
  "engines": {
    "node": ">=25.0.0"
  }
}
